    "import numpy\n",
    "import glob\n",
    "import skimage.io\n",
    "import skimage.transform\n",
    "\n",
    "path = \"./data/images/\"\n",
    "\n",
    "for index,file in enumerate(glob.glob(path+\"*.jpg\")):\n",
    "    image = skimage.io.imread(file)\n",
    "    image = skimage.transform.rescale(image, 0.5)\n",
    "    skimage.io.imsave(file, image)"
